#Задание 1
x=int(input())
if x<0:
    print(x+2*(-x))
elif x==0:
    print(1)

#Задание 2
x=input()
if "." in x or "," in x:
    print(True)
else:
    print(False)

#Задание 3
x=int(input())
y=int(input())
if x%3==0 and y%3==0:
    print(True)
elif x%3==0 or y%3==0:
    print("Одно число делится на 3")
else:
    print(False)
